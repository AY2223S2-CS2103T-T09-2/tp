<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <HBox minWidth="450" prefHeight="16.0" styleClass="pane-person-event-label">
    <children>
      <VBox maxWidth="1.7976931348623157E308" minWidth="283.0" prefHeight="17.0" prefWidth="283.0" HBox.hgrow="ALWAYS">
        <children>
          <Label alignment="CENTER" minWidth="285.0" prefHeight="17.0" prefWidth="285.0" styleClass="label-sub-header" text="Contacts" textFill="WHITE" VBox.vgrow="SOMETIMES" />
        </children>
      </VBox>

      <VBox maxWidth="1.7976931348623157E308" minWidth="182.0" prefHeight="17.0" prefWidth="202.0" HBox.hgrow="ALWAYS">
        <children>
          <Label prefHeight="17.0" prefWidth="195.0" styleClass="label-sub-header" text="Isolated Events" textFill="WHITE" VBox.vgrow="ALWAYS" />
        </children>
      </VBox>

      <VBox maxWidth="1.7976931348623157E308" minWidth="182.0" HBox.hgrow="ALWAYS">
        <children>
          <Label prefHeight="17.0" prefWidth="185.0" styleClass="label-sub-header" text="Recurring Events" textFill="WHITE" VBox.vgrow="ALWAYS" />
        </children>
      </VBox>
    </children>
    <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
  </HBox>
  <ListView fx:id="personListView" VBox.vgrow="ALWAYS" />
</VBox>
